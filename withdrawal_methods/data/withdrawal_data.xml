<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="stock_picking_type_withdrawal" model="stock.picking.type">
            <field name="name">Withdrawal Point</field>
            <field name="code">outgoing</field>
            <field name="sequence_code">WR</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="default_location_src_id" model="stock.location" eval="obj().env.ref('stock.warehouse0').lot_stock_id.id"/>
            <field name="warehouse_id" ref="stock.warehouse0"/>
            <field name="reservation_method">at_confirm</field>
            <field name="use_create_lots" eval="False"/>
        </record>
    </data>
</odoo>
